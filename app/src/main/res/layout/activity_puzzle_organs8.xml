<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgg"
    tools:context="com.example.ss.gameforkids.PuzzleOrgans8">

    <RelativeLayout
        android:layout_width="322dp"
        android:layout_height="482dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.47000003"
        app:layout_constraintHorizontal_bias="0.5">

        <ImageView
            android:id="@+id/imgTendon"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            app:srcCompat="@drawable/tendon"/>

        <ImageView
            android:id="@+id/imv1"
            android:layout_width="50dp"
            android:layout_height="55dp"
            android:layout_below="@+id/textView1"
            android:layout_toLeftOf="@+id/imv2"
            android:layout_toStartOf="@+id/imv2"
            app:srcCompat="@drawable/box"/>

        <ImageView
            android:id="@+id/imv2"
            android:layout_width="50dp"
            android:layout_height="55dp"
            android:layout_alignTop="@+id/imv1"
            android:layout_toLeftOf="@+id/imv3"
            android:layout_toStartOf="@+id/imv3"
            app:srcCompat="@drawable/box"/>

        <ImageView
            android:id="@+id/imv3"
            android:layout_width="50dp"
            android:layout_height="55dp"
            android:layout_alignTop="@+id/imv2"
            android:layout_toLeftOf="@+id/imv4"
            android:layout_toStartOf="@+id/imv4"
            app:srcCompat="@drawable/box"/>

        <ImageView
            android:id="@+id/imv4"
            android:layout_width="50dp"
            android:layout_height="55dp"
            android:layout_alignTop="@+id/imv3"
            android:layout_toLeftOf="@+id/imv5"
            android:layout_toStartOf="@+id/imv5"
            app:srcCompat="@drawable/box"/>

        <ImageView
            android:id="@+id/imv5"
            android:layout_width="50dp"
            android:layout_height="55dp"
            android:layout_alignTop="@+id/imv4"
            android:layout_toLeftOf="@+id/imv6"
            android:layout_toStartOf="@+id/imv6"
            app:srcCompat="@drawable/box"/>

        <ImageView
            android:id="@+id/imv6"
            android:layout_width="50dp"
            android:layout_height="55dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/imv5"
            app:srcCompat="@drawable/box"/>

        <ImageView
            android:id="@+id/imD"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_below="@+id/imgTendon"
            android:layout_toLeftOf="@+id/imgTendon"
            android:layout_toStartOf="@+id/imgTendon"
            app:srcCompat="@drawable/d"/>

        <ImageView
            android:id="@+id/imY"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_below="@+id/imgTendon"
            android:layout_toEndOf="@+id/imgTendon"
            android:layout_toRightOf="@+id/imgTendon"
            app:srcCompat="@drawable/y"/>

        <ImageView
            android:id="@+id/imT"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_alignLeft="@+id/imD"
            android:layout_alignStart="@+id/imD"
            android:layout_alignTop="@+id/imS"
            app:srcCompat="@drawable/t"/>

        <ImageView
            android:id="@+id/imS"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_alignLeft="@+id/imY"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/imY"
            android:layout_marginBottom="16dp"
            app:srcCompat="@drawable/s"/>

        <TextView
            android:id="@+id/textView"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignBottom="@+id/imv1"
            android:layout_toLeftOf="@+id/textView2"
            android:layout_toStartOf="@+id/textView2"
            android:text="T"
            android:textColor="@android:color/black"
            android:textSize="22sp"/>

        <TextView
            android:id="@+id/textView2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignBottom="@+id/imv2"
            android:layout_alignLeft="@+id/imgTendon"
            android:layout_alignStart="@+id/imgTendon"
            android:text="E"
            android:textColor="@android:color/black"
            android:textSize="22sp"/>

        <TextView
            android:id="@+id/textView4"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignBottom="@+id/imv3"
            android:layout_toEndOf="@+id/textView2"
            android:layout_toRightOf="@+id/textView2"
            android:text="N"
            android:textColor="@android:color/black"
            android:textSize="22sp"/>

        <TextView
            android:id="@+id/textView5"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignBottom="@+id/imv5"
            android:layout_alignEnd="@+id/imY"
            android:layout_alignRight="@+id/imY"
            android:text="O"
            android:textColor="@android:color/black"
            android:textSize="22sp"/>

        <TextView
            android:id="@+id/textView6"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignBaseline="@+id/textView5"
            android:layout_alignBottom="@+id/textView5"
            android:layout_toEndOf="@+id/textView5"
            android:layout_toRightOf="@+id/textView5"
            android:text="N"
            android:textColor="@android:color/black"
            android:textSize="22sp"/>

    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
