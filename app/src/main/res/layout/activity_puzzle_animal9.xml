<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgg"
    tools:context="com.example.ss.gameforkids.PuzzleAnimal9">

    <RelativeLayout
        android:layout_width="322dp"
        android:layout_height="482dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent">

        <ImageView
            android:id="@+id/imgZebra"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            app:srcCompat="@drawable/zebra" />

        <ImageView
            android:id="@+id/imv1"
            android:layout_width="65dp"
            android:layout_height="70dp"
            app:srcCompat="@drawable/box"
            android:layout_marginTop="44dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:id="@+id/imv2"
            android:layout_width="65dp"
            android:layout_height="70dp"
            android:layout_alignTop="@+id/imv1"
            android:layout_toEndOf="@+id/imv1"
            android:layout_toRightOf="@+id/imv1"
            app:srcCompat="@drawable/box" />

        <ImageView
            android:id="@+id/imv3"
            android:layout_width="65dp"
            android:layout_height="70dp"
            app:srcCompat="@drawable/box"
            android:layout_alignTop="@+id/imv2"
            android:layout_centerHorizontal="true" />

        <ImageView
            android:id="@+id/imv4"
            android:layout_width="65dp"
            android:layout_height="70dp"
            app:srcCompat="@drawable/box"
            android:layout_alignTop="@+id/imv3"
            android:layout_toRightOf="@+id/imv3"
            android:layout_toEndOf="@+id/imv3" />

        <ImageView
            android:id="@+id/imv5"
            android:layout_width="65dp"
            android:layout_height="70dp"
            app:srcCompat="@drawable/box"
            android:layout_alignTop="@+id/imv4"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
        <TextView
            android:id="@+id/textView2"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:text="E"
            android:textColor="@android:color/black"
            android:textSize="28sp"
            android:layout_alignTop="@+id/imv2"
            android:layout_alignRight="@+id/imv2"
            android:layout_alignEnd="@+id/imv2"
            android:layout_marginTop="14dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:text="B"
            android:textColor="@android:color/black"
            android:textSize="28sp"
            android:layout_alignBaseline="@+id/textView4"
            android:layout_alignBottom="@+id/textView4"
            android:layout_toLeftOf="@+id/imv4"
            android:layout_toStartOf="@+id/imv4" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:text="R"
            android:textColor="@android:color/black"
            android:textSize="28sp"
            android:layout_alignBaseline="@+id/textView2"
            android:layout_alignBottom="@+id/textView2"
            android:layout_toLeftOf="@+id/imv5"
            android:layout_toStartOf="@+id/imv5" />
        <TextView
            android:id="@+id/textView5"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:text="A"
            android:textSize="28sp"
            android:textColor="@android:color/black"
            android:layout_alignBaseline="@+id/textView4"
            android:layout_alignBottom="@+id/textView4"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <ImageView
            android:id="@+id/imS"
            android:layout_width="70dp"
            android:layout_height="70dp"
            app:srcCompat="@drawable/s"
            android:layout_below="@+id/imgZebra"
            android:layout_toLeftOf="@+id/imgZebra"
            android:layout_toStartOf="@+id/imgZebra" />

        <ImageView
            android:id="@+id/imC"
            android:layout_width="70dp"
            android:layout_height="70dp"
            app:srcCompat="@drawable/c"
            android:layout_below="@+id/imgZebra"
            android:layout_alignLeft="@+id/imF"
            android:layout_alignStart="@+id/imF" />

        <ImageView
            android:id="@+id/imZ"
            android:layout_width="70dp"
            android:layout_height="70dp"
            app:srcCompat="@drawable/z"
            android:layout_below="@+id/imS"
            android:layout_alignLeft="@+id/imS"
            android:layout_alignStart="@+id/imS" />

        <ImageView
            android:id="@+id/imF"
            android:layout_width="70dp"
            android:layout_height="70dp"
            app:srcCompat="@drawable/f"
            android:layout_below="@+id/imC"
            android:layout_toRightOf="@+id/imgZebra"
            android:layout_toEndOf="@+id/imgZebra" />

    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
